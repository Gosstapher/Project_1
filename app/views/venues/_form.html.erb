<%= form_for @venue do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.submit "create venue" %>
<% end %>