<h1><%= @user.name %></h1>
<div>
  <p><%= @user.email %></p>
  <br>
  <p><% @user.bookings.each do |booking| %>
    <%= link_to("#{booking.id}", booking_path(booking)) %>
    <br>
    <% end %>



  </p>

</div>

<%= link_to("Edit #{@user.name}", edit_user_path(@user)) %><br>
<%= button_to("Delete #{@user.name} ?", user_path(@user), method: :delete, data: {confirm: 'Delete #{@user.name}? Are you sure?!'}) %>